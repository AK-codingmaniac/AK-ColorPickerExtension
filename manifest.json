{
    "name" : "AK Color Picker",
    "description" : "Basic color picker chrome extension",
    "version" : "1.0",
    "manifest_version": 3,
    "background" : {
        "service_worker": "background.js"
    },
    "permissions" : [
        "storage",
        "activeTab",
        "scripting"
    ],
    "action" :{
        "default_popup" : "popup.html"
    },
    "icons": {
        "128":"logofinal.png"
    }
}